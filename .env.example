# PORT
PORT=80

# DB
DB_HOST=localhost
DB_PORT=5432
DB_USERNAME=user
DB_PASSWORD=1234
DB_DATABASE=creverse

# AZURE Blob Storage
AZURE_ACCOUNT_NAME=devtaskblob
